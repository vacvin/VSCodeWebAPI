@{
    ViewData["Title"] = "Index";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>User Management</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
	<script src="js/index.js"></script>
    <link rel="stylesheet" href="css/index.css">
</head>
<body>
    <div id="app">
        User List:<br/>
        Query By ID:<input v-model="ID" v-on:keyup="QueryUser"><br/>
        <userlist-header></userlist-header>
        <userlist
            v-for="user in users"
            v-bind:user="user"
            v-bind:key="user.id">
        </userlist>       
        <div>
            <div>
                <div class="input">Name</div><div class="input"><input v-model="name"></div>
            </div>
            <div>
                <div class="input">Birthday</div><div class="input"><input v-model="birthday"></div>
            </div>
            <div>
                <button class="input" v-on:click="addUser">Add User</button>
            </div>
        </div>        
    </div>
</body>
</html>